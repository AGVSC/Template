{
    "better-comments.tags": [
        {
            "tag": "!",
            "color": "#FF2D00",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "?",
            "color": "#3498DB",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "///",
            "color": "#474747",
            "strikethrough": true,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "todo",
            "color": "#FF8C00",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "*",
            "color": "#98C379",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "Deprecated",
            "color": "#C38613",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": true,
            "italic": false
        },
        {
            "tag": "DEP",
            "color": "#C38613",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": true,
            "italic": false
        }
    ],
    "better-comments.highlightPlainText": true,
    "[cpp]": {
        "editor.defaultFormatter": "xaver.clang-format"
    },
    "cpplint.headers": [
        "h++",
        "cuh",
        "hxx",
        "hpp",
        "h"
    ],
    "cpplint.filters": [
        "-build/header_guard",
        "-build/include_subdir",
        "-runtime/reference",
        "-runtime/int"
    ],
    "C_Cpp.inactiveRegionOpacity": null,
    "C_Cpp.formatting": "clangFormat",
    "C_Cpp.clang_format_sortIncludes": false,
    "clang-format.language.cpp.fallbackStyle": "Google",
    "clang-format.language.cpp.style": "Google",
    "clang-format.language.c.fallbackStyle": "Google",
    "clang-format.language.c.style": "Google",
    "clang-format.language.cuda.fallbackStyle": "Google",
    "clang-format.language.cuda.style": "Google",
    "clang-format.fallbackStyle": "Google",
    "clang-format.style": "Google",
    "doxdocgen.generic.authorName": "Eric Liu",
    "doxdocgen.generic.authorEmail": "ericcholiu@hotmail.com",
    "doxdocgen.file.versionTag": "@version 0.1.0",
    "doxdocgen.file.copyrightTag": [
        "@copyright Copyright (c) {year} QDU-IFF, AGVSC"
    ],
    "python.pythonPath": "/usr/bin/python3",
    "python.formatting.provider": "yapf",
    "python.formatting.yapfArgs": [
        "--style={based_on_style: yapf, indent_width: 4, column_limit: 120}"
    ],
    "python.linting.enabled": true,
    "python.linting.pylintEnabled": true,
    "python.linting.lintOnSave": true,
    "python.linting.pylintArgs": [],
    "pylint.logLevel": "debug",
    "pylint.showNotifications": "always",
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": "active",
    "ros.distro": "noetic",
    "keep-a-changelog.defaultAuthor": "Eric Liu",
    "python.autoComplete.extraPaths": [
        "/opt/ros/noetic/lib/python3/dist-packages"
    ],
}